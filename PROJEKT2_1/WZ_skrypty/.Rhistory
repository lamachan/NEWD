# Your data
df <- data.frame(
"Rodzaj" = c("Jazda chorągwi", "Jazda chorągwi", "Jazda chorągwi", "Jazda chorągwi", "Jazda chorągwi", "Dragonia", "Piechota", "Węgrzy i janczarzy", "Obsługa artylerii"),
"Podrodzaj" = c("husaria", "arkebuzerzya", "pancerni", "lekka jazda", "kozacy", "nieokreślony", "nieokreślony", "nieokreślony", "nieokreślony"),
"Liczba_jednostek" = c(23, 3, 53, 26, 1, 7, 21, 3, 0),
"Liczba_żołnierzy" = c(2670, 530, 5100, 1900, 150, 2800, 7150, 500, 150)
)
# Reshape the data for plotting
df_long <- df %>%
pivot_longer(cols = c("Liczba_jednostek", "Liczba_żołnierzy"), names_to = "Type", values_to = "Value")
# Plot
ggplot(df_long, aes(x = Rodzaj, y = Value, fill = Podrodzaj)) +
geom_bar(stat = "identity", position = "stack") +
facet_wrap(~Type, scales = "free_y") +
labs(title = "Stacked Barplot of Military Units",
x = "Rodzaj",
y = "Value") +
scale_fill_brewer(palette = "Set3") +
theme_minimal()
